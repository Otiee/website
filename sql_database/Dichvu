CREATE TABLE Table_FormDichVu(
    ID INT AUTO_INCREMENT,
    MaDichVu VARCHAR(2),
    FOREIGN KEY (MaDichVu) REFERENCES Table_DichVu(MaDichVu),
    Email VARCHAR(100),
    HoTen VARCHAR(100),
    Phone INT(12),
    TrangThai BIT DEFAULT 0,
    PRIMARY KEY (ID)
);
